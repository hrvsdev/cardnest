{
	"semi": true,
	"bracketSpacing": true,
	"arrowParens": "always",
	"trailingComma": "none",
	"plugins": [
		"@ianvs/prettier-plugin-sort-imports"
	],
	"importOrder": [
		"^react$",
		"^react-dom$",
		"",
		"<THIRD_PARTY_MODULES>",
		"",
		"^[./]",
		"^@components/(.*)$",
		"",
		"^@hooks/(.*)$",
		"^@utils/(.*)$",
		"",
		"^@types/(.*)$"
	],
	"importOrderSeparation": true
}
