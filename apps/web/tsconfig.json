{
	"extends": "../../tsconfig",
	"compilerOptions": {
		"baseUrl": "src",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"jsx": "react-jsx",
		"moduleResolution": "bundler",
		"paths": {
			"@components/*": ["components/*"],
			"@utils/*": ["utils/*"],
			"@pages/*": ["pages/*"],
			"@hooks/*": ["hooks/*"],
			"@t/*": ["types/*"]
		}
	},
	"include": ["src"]
}
